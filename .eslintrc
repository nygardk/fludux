{
  "extends": "eslint-config-airbnb",
  "env": {
    "es6": true,
    "jasmine": true
  },
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    "no-unused-vars": 1,
    "comma-dangle": 1,

    "react/jsx-closing-bracket-location": 0,
    "react/prefer-es6-class": 0,
    "react/no-multi-comp": 0,
    "prefer-arrow-callback": 0
  },
  "plugins": [
    "react"
  ]
}
